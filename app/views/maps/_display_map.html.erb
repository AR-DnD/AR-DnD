<% grid_dimension = 6%>
<table class="table table-mapgrid-mini">
  <tbody>
    <% data = JSON.parse map.data%>
    <% (0..(grid_dimension-1)).each do |i| %>
      <tr>
        <% (0..(grid_dimension-1)).each do |j| %>
          <td>
            <% currElement = data[i][j].to_s %>
            <% image_string = currElement != '' ? 'url(/assets/' + currElement.split('-')[1] + '.png);' : 'nil' %>
            <button type="button"  class="btn-mapgrid-mini" style = "background-image: <%= image_string %>"  ></button>
          </td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
